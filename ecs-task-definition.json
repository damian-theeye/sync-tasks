{
    "family": "theeye-api-sync",
    "executionRoleArn": "arn:aws:iam::174383185918:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "theeye-sync-tasks",
            "image": "174383185918.dkr.ecr.us-east-1.amazonaws.com/sync-tasks:latest",
            "portMappings": [
                {
                    "containerPort": 6081,
                    "hostPort": 6081,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
